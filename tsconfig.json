{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@models/*": [
        "./models/*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@screens/*": [
        "./screens/*"
      ],
      "@utils/*": [
        "./utils/*"
      ],
      "@assets/*": [
        "./assets/*"
      ],
      "@navigation/*": [
        "./navigation/*"
      ],
      "@services/*": [
        "./services/*"
      ],
      "@hooks/*": [
        "./hooks/*"
      ],
      "@i18n/*": [
        "./i18n/*"
      ],
      "@styles/*": [
        "./styles/*"
      ],
      "@theme/*": [
        "./theme/*"
      ],
      "@store/*": [
        "./store/*"
      ],
      "@constants/*": [
        "./constants/*"
      ],
      "@thunks/*": [
        "./thunks/*"
      ],
    },
  },
}